<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="377.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.organon.controller.TelaProjetoController">
   <children>
      <VBox alignment="CENTER" prefHeight="400.0" prefWidth="318.0" spacing="10.0" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0">
         <children>
            <TextField fx:id="txtNomeProjeto" promptText="Nome" />
            <TextField fx:id="txtLinguagens" promptText="Linguagens de programação" />
            <TextField fx:id="txtLinkRepositorio" promptText="Link do repositorio" />
            <TextField fx:id="txtModProcesso" promptText="Modelo de processo" />
            <TextArea fx:id="txtDescricao" prefHeight="200.0" prefWidth="200.0" promptText="Descrição" />
            <HBox alignment="TOP_RIGHT" prefHeight="25.0" prefWidth="367.0" spacing="5.0">
               <children>
                  <ChoiceBox fx:id="choiseBoxEscolherProjeto" prefHeight="25.0" prefWidth="184.0" />
                  <Button fx:id="btnEditarProjeto" mnemonicParsing="false" onAction="#funcaoEditarProjeto" prefHeight="25.0" prefWidth="58.0" text="Editar" />
                  <Button fx:id="bntExcluirProjeto" mnemonicParsing="false" onAction="#funcaoExcluirProjeto" text="Excluir" />
                  <Button fx:id="btnSalvarProjeto" mnemonicParsing="false" onAction="#funcaoSalvarProjeto" text="Salvar" />
               </children>
            </HBox>
         </children>
         <opaqueInsets>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </opaqueInsets>
      </VBox>
   </children>
</AnchorPane>
