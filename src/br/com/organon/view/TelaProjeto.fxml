<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="377.0" style="-fx-background-color: #536c8d;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.organon.controller.TelaProjetoController">
