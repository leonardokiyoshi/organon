         <children>
            <TextField fx:id="txtNomeProjeto" promptText="Nome" stylesheets="@csstrick.css" />
            <TextField fx:id="txtLinguagens" promptText="Linguagem de programação" stylesheets="@csstrick.css" />
            <TextField fx:id="txtLinkRepositorio" promptText="Link do repositório" stylesheets="@csstrick.css" />
            <TextField fx:id="txtModProcesso" promptText="Modelo de processo" stylesheets="@csstrick.css" />
            <TextArea fx:id="txtDescricao" prefHeight="186.0" prefWidth="283.0" promptText="Descrição" stylesheets="@csstrick.css" />
            <HBox alignment="TOP_RIGHT" prefHeight="25.0" prefWidth="367.0" spacing="5.0">
               <children>
                  <ChoiceBox fx:id="choiseBoxEscolherProjeto" prefHeight="26.0" prefWidth="204.0" />
